<!-- Begin page content -->
<div class="container">
  <div class="page-header">
    <h1>Programacion Auxiliares</h1>
  </div>
  <form id="uploadForm"
        enctype="multipart/form-data"
        action="/calendario/upload"
        method="POST">
    <div class="form-group">
      <label>Archivo Excel (.xls)</label>
      <!-- Seleccionar archivo <input type="file" name="calendarioCursos" id="calendarioCursos" /> -->
      <span id="seleccionar" class="btn btn-primary btn-file btn-xs">
          Seleccionar archivo <input type="file" name="calendarioCursos" id="calendarioCursos" accept="application/vnd.ms-excel"/>
      </span>

      <span id="filename" class="alert alert-info" hidden></span>
      
    </div>
      
    <button type="submit" class="btn btn-success" name="submit">Enviar</button>
  </form>
</div>

<footer class="footer">
  <div class="container">
    <p class="text-muted">Place sticky footer content here.</p>
  </div>
</footer>